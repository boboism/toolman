- model_class = @tunning_diagram.class
.row-fluid.page-header
  .span5
    %h3= t(".title", title: "#{model_class.model_name.human}: #{@tunning_diagram.id}", default: t("helpers.views.edit.title", title: "#{model_class.model_name.human}: #{@tunning_diagram.id}"))
  .span7
    .pull-right
      .btn-group
        = link_to tunning_diagrams_path, class: "btn" do
          %i.icon-list
          %span= t(".index", default: t("helpers.links.index"))
      .btn-group
        = render "layouts/toolbar"
.row-fluid
  .span12
    = simple_form_for(@tunning_diagram, html: {class: "well form-horizontal"}) do |f|
      = f.error_notification
      = display_base_errors @tunning_diagram
      .form-inputs
        = f.file_field :archive

      .form-actions
        = f.button :submit, t('helpers.links.submit'), class: 'btn-primary'
