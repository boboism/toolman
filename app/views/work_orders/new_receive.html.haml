- model_class = @work_order.class
.row-fluid.page-header
  .span5
    %h3= t(".title")
  .span7
    .pull-right
      .btn-group
        = link_to receives_work_orders_path, class: "btn" do
          %i.icon-list
          %span= t(".index", default: t("helpers.links.index"))
      .btn-group
        = render "layouts/toolbar"
.row-fluid
  .span12
    = render "receive"

:javascript
  $(function() {
    auto_submit_receive();
  });
